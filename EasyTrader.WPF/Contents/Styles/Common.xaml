<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:EasyTrader.WPF.Contents.Styles">
    
    <Color x:Key="colorPrimary1">#799540</Color>
    <Color x:Key="colorPrimary2">#50632b</Color>

    <SolidColorBrush x:Key="solidBrush1" Color="{StaticResource colorPrimary1}" />
    <SolidColorBrush x:Key="solidBrush2" Color="{StaticResource colorPrimary2}" />
    
</ResourceDictionary>